<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	 <Description>Library Helper to make easier code.</Description>
    <Copyright>GPL</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="BeetleX.dll" />
    <None Remove="BeetleX.Http.Clients.dll" />
    <None Remove="MySql.Data.dll" />
    <None Remove="Newtonsoft.Json.dll" />
    <None Remove="Newtonsoft.Json.xml" />
    <None Remove="Ubiety.Dns.Core.dll" />
    <None Remove="Zstandard.Net.dll" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="BeetleX.dll">
